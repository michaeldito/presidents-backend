  "main": "index.js",
  "scripts": {
    "test": "export NODE_ENV=dev && mocha test/*js",
    "jest": "export NODE_ENV=dev && jest",
    "test-models": "export NODE_ENV=dev && mocha test/*model.js",
    "test-utils": "export NODE_ENV=dev && mocha test/*utils.js",
    "test-proc": "export NODE_ENV=dev && mocha --timeout 15000 test/*processing.js",
    "test-server": "export NODE_ENV=dev && mocha --timeout 15000 test/*server.js",
    "test-play": "export NODE_ENV=dev && mocha --timeout 15000 test/*playground.js",
    "count-src": "find src -name '*.js' | xargs wc -l",
    "count-test": "find test -name '*.js' | xargs wc -l",
    "start": "export NODE_ENV=dev && nodemon src/server/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.4.5",
    "assert": "^2.0.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^4.0.0",
    "expect": "^24.8.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.1.0",
    "koa-jwt": "^3.3.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.3.0",
    "koa-socket": "^4.4.0",
