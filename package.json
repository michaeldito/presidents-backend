{
  "name": "larry-presidents-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=prod && nodemon --exec babel-node src/server/index.js",
    "test": "export NODE_ENV=dev && mocha --require @babel/register --require babel-polyfill --timeout 15000 --exit src/tests/*",
    "test:unit": "export NODE_ENV=dev && mocha --require @babel/register --require babel-polyfill --timeout 15000 --exit src/tests/unit* -- $*",
    "test:system": "export NODE_ENV=dev && mocha --require @babel/register --require babel-polyfill --timeout 15000 --exit src/tests/system*",
    "count:src": "find src -name '*.js' | xargs wc -l",
    "start:dev": "export NODE_ENV=dev && export PORT=8080 && nodemon src/server/index.js",
    "db:init-prod": "export DB_ACTION=init && export NODE_ENV=prod && node src/tests/db.js",
    "db:init-dev": "export DB_ACTION=init && export NODE_ENV=dev && node src/tests/db.js",
    "db:drop-prod": "export DB_ACTION=drop && export NODE_ENV=prod && node src/tests/db.js",
    "db:drop-dev": "export DB_ACTION=drop && export NODE_ENV=dev && node src/tests/db.js",
    "db:command": "export DB_ACTION=command && export NODE_ENV=dev && node src/tests/db.js",
    "create-module": "node src/playground/module.js -- $*",
    "clean": "rm -r node_modules; rm -r log",
    "lint": "eslint --fix \"src/**/*.js\"",
    "docker:build": "docker build . -t prez-api:1.0",
    "docker:run": "docker container run -p 8080:8080 --detach --name prez-api prez-api:1.0",
    "docker:remove": "docker container rm --force prez-api",
    "kube:deploy": "kubectl apply -f prez-api.yaml",
    "kube:get": "kubectl get services",
    "kube:delete": "kubectl delete -f prez-api.yaml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assert": "^2.0.0",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "dotenv": "^4.0.0",
    "expect": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.1.0",
    "koa-jwt": "^3.3.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.3.0",
    "mongoose": "^5.5.9",
    "mongoose-autopopulate": "^0.9.1",
    "mongoose-id-validator": "^0.5.3",
    "mongoose-unique-validator": "^2.0.3",
    "nodemon": "^1.19.4",
    "should": "^13.2.3",
    "socket.io": "^2.3.0",
    "twilio": "^3.37.1",
    "winston": "^3.2.1"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
